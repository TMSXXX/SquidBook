{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "墨鱼记账",
  "version": "0.1.0",
  "identifier": "com.squidbook.octopus",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "墨鱼记账",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
        
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [".account_book.db"],
    "windows": {
      "wix": {
        "language": "zh-CN"
      }
    }
  }
}
